{
    "name": "irazasyed/php-witai",
    "type": "library",
    "description": "PHP Library for Wit.ai API with Laravel & Lumen Support out of the box!",
    "keywords": [
        "irazasyed",
        "php-witai",
        "wit",
        "wit.ai",
        "witai",
        "php-wit",
        "laravel",
        "lumen",
        "laravel wit",
        "lumen wit"
    ],
    "homepage": "https://github.com/irazasyed/php-witai",
    "license": "MIT",
    "authors": [
        {
            "name": "Syed Irfaq R.",
            "email": "syed+gh@lukonet.com",
            "homepage": "https://lukonet.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : "~5.5|~7.0",
        "guzzlehttp/guzzle": "~6.0",
        "illuminate/support": "~5.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3"
    },
    "autoload": {
        "psr-4": {
            "Irazasyed\\Wit\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Irazasyed\\Wit\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
